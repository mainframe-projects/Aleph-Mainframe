  FROM ENVIRONMENT 'DEVELOP' SYSTEM 'ICD' SUBSYSTEM 'TESTFONT'
    TYPE 'PIDMSCB3' STAGE U                                  
  OPTIONS CCID 'ICDTEST'                                      
